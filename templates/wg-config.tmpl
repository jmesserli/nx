#
# Wireguard config generated by nx (https://github.com/jmesserli/nx)
# Start of config for VPN server {{ .ServerName }}
#

[Interface]
PrivateKey = #PRIVATEKEY#
Address = {{ .OwnAddress }}
ListenPort = {{ .OwnPort }}
# do not create routing table entries
Table = off


{{ range $peer := .Peers -}}
[Peer]
# Peer {{ $peer.Name }}
Endpoint = {{ $peer.IP }}:{{ $peer.Port }}
PublicKey = {{ $peer.PublicKey }}
AllowedIps = 0.0.0.0/0
PersistentKeepalive = 25

{{ end -}}
#
# End of config for VPN server {{ .ServerName }}
#